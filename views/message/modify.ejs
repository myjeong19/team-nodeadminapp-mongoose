<!--우측 콘텐츠 영역 -->
<form action="/member/create" method="post">
  <div class="wrapper wrapper-content">
    <div class="ibox-content m-b-sm border-bottom">
      <div class="row">
        <div class="col-sm-6">
          <div class="form-group">
            <div>
              <label class="control-label">채널 고유번호</label>
              <input type="text" class="form-control mb-3" />
            </div>
            <div class="form-group">
              <label class="control-label col-6">사용자 고유번호</label>
              <input type="text" class="form-control mb-3" />
            </div>
            <div class="form-group">
              <label class="control-label">사용자 닉네임</label>
              <input type="text" class="form-control" />
            </div>
            <div class="form-group">
              <label class="control-label">사용자 유형</label>
              <input type="text" class="form-control" />
            </div>
            <div class="form-group">
              <label class="control-label">최근 아웃 일시</label>
              <input type="text" class="form-control" />
            </div>
            <div class="form-group">
              <label class="control-label">커넥션 아이디</label>
              <input type="text" class="form-control" />
            </div>
            <div class="form-group">
              <label class="control-label">아이피 주소</label>
              <input type="text" class="form-control" />
            </div>
            <div class="form-group">
              <label class="control-label">수정일시</label>
              <input type="text" class="form-control" />
            </div>

            <div class="form-group">
              <label class="control-label">수정자 고유번호</label>
              <input type="text" class="form-control" />
            </div>
          </div>
        </div>
      </div>

      <div class="text-center">
        <button type="submit" class="btn btn-primary">저장</button>
        <a href="/member/list" class="btn btn-info">목록</a>
      </div>
    </div>
  </div>
</form>
